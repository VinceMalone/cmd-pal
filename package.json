{
  "name": "cmd-pal",
  "version": "0.0.0",
  "description": "React Command Palette",
  "main": "dist/bundle.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "lint:format": "prettier --check *.{js,ts,tsx,json,css,md}",
    "lint:es": "eslint --ext .js,.ts,.tsx .",
    "lint": "yarn lint:format && yarn lint:es",
    "test": "jest",
    "test:watch": "jest --watch",
    "typecheck": "tsc --noEmit",
    "declaration": "tsc --emitDeclarationOnly --jsx preserve --declarationDir dist --declaration src/index.ts",
    "storybook": "start-storybook",
    "start": "webpack-dev-server --mode=development",
    "build": "webpack --config webpack.config.production.js",
    "build-docs": "webpack --config webpack.config.js && mkdir -p docs && cp dist/main.js docs/main.js && cp public/index.html docs/index.html",
    "clean": "rimraf dist",
    "prepublishOnly": "yarn clean && yarn build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/VinceMalone/cmd-pal.git"
  },
  "author": "VinceMalone <vince.t.malone@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/VinceMalone/cmd-pal/issues"
  },
  "homepage": "https://github.com/VinceMalone/cmd-pal/#readme",
  "engines": {
    "node": ">=10.x",
    "yarn": "^1.x"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{json,md}": [
      "prettier --write"
    ],
    "*.{js,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "peerDependencies": {
    "react": "^16.9.0",
    "react-dom": "^16.9.0"
  },
  "dependencies": {
    "hotkeys-js": "^3.7.3",
    "react-is": "^16.13.0",
    "react-use": "^13.27.0",
    "react-virtualized-auto-sizer": "^1.0.2",
    "react-window": "^1.8.5",
    "styled-components": "^5.0.1",
    "ts-toolbelt": "^6.3.6",
    "use-resolved": "^0.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.8.3",
    "@babel/plugin-proposal-optional-chaining": "^7.8.3",
    "@babel/preset-env": "^7.8.7",
    "@storybook/addon-docs": "^5.3.17",
    "@storybook/addon-info": "^5.3.17",
    "@storybook/preset-typescript": "^2.1.0",
    "@storybook/react": "^5.3.17",
    "@types/faker": "^4.1.10",
    "@types/react": "^16.9.23",
    "@types/react-dom": "^16.9.5",
    "@types/react-virtualized-auto-sizer": "^1.0.0",
    "@types/react-window": "^1.8.1",
    "@types/styled-components": "^5.0.1",
    "@typescript-eslint/eslint-plugin": "^2.23.0",
    "@typescript-eslint/parser": "^2.23.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-styled-components": "^1.10.7",
    "csvtojson": "^2.0.10",
    "eslint": "^6.8.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^2.5.0",
    "faker": "^4.1.0",
    "husky": "^4.2.3",
    "lint-staged": "^10.0.8",
    "prettier": "^1.19.1",
    "react": "^16.13.0",
    "react-docgen-typescript-loader": "^3.7.1",
    "react-dom": "^16.13.0",
    "rimraf": "^3.0.2",
    "style-loader": "^1.1.3",
    "ts-loader": "^6.2.1",
    "typescript": "^3.8.3",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-merge": "^4.2.2"
  }
}
